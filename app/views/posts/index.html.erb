<% content_for(:title, "投稿スポット一覧") %>
<section id="spots">
  <div class="container aos-init aos-animate" data-aos="fade-up">
    <div class="section-header">
      <h2 class="mt-5">Spot Index</h2>
      <p>投稿スポット一覧</p>
    </div>
    <%= render 'shared/posts/search_form' %>
    <%= render 'shared/posts/posts_list', posts: @posts, posts_count: @posts_count, current_user: @current_user %>
  </div>
</section>
